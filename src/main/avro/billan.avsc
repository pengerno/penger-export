  "doc": "Informasjon på billånssøknad",
  "fields": [
    {
      "name": "kundereferanse",
      "type": "string"
    },
    {
      "name": "profiler",
      "doc": "Informasjon om låntaker, medlåntaker og andre medlemmer av husholdningen",
